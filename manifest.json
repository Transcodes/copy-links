{
   "name": "Copy Links",
   "version": "1.3.0",
   "description": "Copy Links clone for Chrome",
   "manifest_version": 2,
   "minimum_chrome_version": "26",
   "background": {
      "page": "background.html",
      "persistent": true
   },
   "browser_action": {
      "default_icon": "icon48.png",
      "default_title": "Copy Links",
      "default_popup": "popup.html"
   },
   "icons": {
      "128": "icon128.png",
      "48": "icon48.png"
   },
   "permissions": [ "activeTab" ]
}
